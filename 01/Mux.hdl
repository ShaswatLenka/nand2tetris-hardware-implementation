/** 
 * Multiplexor:
 * out = a if sel == 0
 *       b otherwise
 */

CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    // Put your code here:
    Not(in=sel,out=notsel);
    Or(a=a,b=sel,out=c);
    Or(a=b,b=notsel,out=d);
    And(a=c,b=d,out=out);

}